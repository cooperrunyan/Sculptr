{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build test",
      "type": "shell",
      "command": "rm -rf ./TEST && mkdir ./TEST && cd ./TEST && deno run --unstable --allow-write --allow-read --allow-run --allow-env ../src/index.ts build react . --ts --scss --skip"
    },
    {
      "label": "Add test",
      "type": "shell",
      "command": "rm -rf ./TEST && mkdir ./TEST && cd ./TEST && deno run --unstable --allow-read --allow-write --allow-run --allow-env ../src/index.ts add license ncsa"
    },
    {
      "label": "Assets",
      "type": "shell",
      "command": "deno run --unstable --allow-read --allow-write --allow-env src/generateAssets.ts"
    },
    {
      "label": "Update",
      "type": "shell",
      "command": "deno uninstall sculptr && deno install --unstable --allow-write --allow-read --allow-net --allow-run -n sculptr --allow-env -f https://deno.land/x/sculptr/src/index.ts"
    }
  ]
}
